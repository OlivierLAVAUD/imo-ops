{
  "site": {
    "name": "IAD France",
    "base_url": "https://www.iadfrance.fr",
    "search_url": "https://www.iadfrance.fr/annonces/vente",
    "wait_timeout": 10000,
    "navigation_delay": 2000
  },

  "navigation_menu": {
    "container": {
      "css_selector": "nav.flex-1",
      "xpath": "//nav[contains(@class, 'flex-1')]",
      "description": "Menu de navigation principal"
    },
    "items": {
      "acheter": {
        "css_selector": "button span:has-text('Acheter')",
        "xpath": "//button[.//span[contains(., 'Acheter')]]",
        "description": "Menu Acheter"
      },
      "louer": {
        "css_selector": "button span:has-text('Louer')",
        "xpath": "//button[.//span[contains(., 'Louer')]]",
        "description": "Menu Louer"
      },
      "vendre_estimer": {
        "css_selector": "button span:has-text('Vendre / Estimer')",
        "xpath": "//button[.//span[contains(., 'Vendre / Estimer')]]",
        "description": "Menu Vendre / Estimer"
      },
      "trouver_conseiller": {
        "css_selector": "a[href*='/trouver-un-conseiller']",
        "xpath": "//a[contains(@href, '/trouver-un-conseiller')]",
        "description": "Lien Trouver un conseiller"
      },
      "decouvrir_iad": {
        "css_selector": "button span:has-text('Découvrir iad')",
        "xpath": "//button[.//span[contains(., 'Découvrir iad')]]",
        "description": "Menu Découvrir iad"
      },
      "devenir_conseiller": {
        "css_selector": "a[href*='join-iad.fr']",
        "xpath": "//a[contains(@href, 'join-iad.fr')]",
        "description": "Lien Devenir conseiller"
      },
      "langue": {
        "css_selector": "button#reka-dropdown-menu-trigger-v-15-0",
        "xpath": "//button[@id='reka-dropdown-menu-trigger-v-15-0']",
        "description": "Sélecteur de langue"
      }
    }
  },
  
  "search_form": {
    "form_container": {
      "css_selector": "form.relative",
      "xpath": "//form[@class='relative']",
      "description": "Formulaire principal de recherche"
    },
    
    "transaction_type": {
      "description": "Type de transaction (Acheter/Louer/Vendre)",
      "container_css": "form.relative > div > div > fieldset.inline-flex",
      "container_xpath": "//form[@class='relative']//fieldset[contains(@class, 'inline-flex')]",
      "options": {
        "acheter": {
          "css_selector": "label.flex-1:nth-child(1)",
          "xpath": "//label[contains(@class, 'flex-1') and contains(., 'Acheter')]",
          "input_css": "label.flex-1:nth-child(1) > input",
          "input_xpath": "//label[contains(., 'Acheter')]//input[@type='radio']",
          "checked": true,
          "description": "Option Acheter (sélectionnée par défaut)"
        },
        "louer": {
          "css_selector": "label.flex-1:nth-child(2)",
          "xpath": "//label[contains(@class, 'flex-1') and contains(., 'Louer')]",
          "input_css": "label.flex-1:nth-child(2) > input",
          "input_xpath": "//label[contains(., 'Louer')]//input[@type='radio']",
          "description": "Option Louer"
        },
        "vendre": {
          "css_selector": "label.flex-1:nth-child(3)",
          "xpath": "//label[contains(@class, 'flex-1') and contains(., 'Vendre')]",
          "input_css": "label.flex-1:nth-child(3) > input",
          "input_xpath": "//label[contains(., 'Vendre')]//input[@type='radio']",
          "description": "Option Vendre / Estimer"
        }
      }
    },
    
    "type_bien": {
      "description": "Sélection du type de bien immobilier",
      "container_css": ".iad-radio-group-scrollable fieldset.inline-flex",
      "container_xpath": "//div[contains(@class, 'iad-radio-group-scrollable')]//fieldset[contains(@class, 'inline-flex')]",
      "options": {
        "ancien": {
          "css_selector": ".flex-nowrap > label:nth-child(1)",
          "xpath": "//label[contains(., 'Ancien')]",
          "input_css": ".flex-nowrap > label:nth-child(1) > input",
          "input_xpath": "//label[contains(., 'Ancien')]//input[@type='radio']",
          "checked": true,
          "description": "Bien ancien"
        },
        "neuf": {
          "css_selector": ".flex-nowrap > label:nth-child(2)",
          "xpath": "//label[contains(., 'Neuf')]",
          "input_css": ".flex-nowrap > label:nth-child(2) > input",
          "input_xpath": "//label[contains(., 'Neuf')]//input[@type='radio']",
          "description": "Bien neuf"
        },
        "prestige": {
          "css_selector": "label.flex:nth-child(3)",
          "xpath": "//label[contains(., 'Prestige')]",
          "input_css": "label.flex:nth-child(3) > input",
          "input_xpath": "//label[contains(., 'Prestige')]//input[@type='radio']",
          "description": "Bien prestige"
        },
        "international": {
          "css_selector": "label.border:nth-child(4)",
          "xpath": "//label[contains(., 'International')]",
          "input_css": "label.border:nth-child(4) > input",
          "input_xpath": "//label[contains(., 'International')]//input[@type='radio']",
          "description": "Bien international"
        },
        "terrain": {
          "css_selector": "label.border:nth-child(5)",
          "xpath": "//label[contains(., 'Terrain')]",
          "input_css": "label.border:nth-child(5) > input",
          "input_xpath": "//label[contains(., 'Terrain')]//input[@type='radio']",
          "description": "Terrain"
        },
        "entreprises_commerces": {
          "css_selector": "label.border:nth-child(6)",
          "xpath": "//label[contains(., 'Entreprises et commerces')]",
          "input_css": "label.border:nth-child(6) > input",
          "input_xpath": "//label[contains(., 'Entreprises et commerces')]//input[@type='radio']",
          "description": "Entreprises et commerces"
        },
        "immeuble": {
          "css_selector": "label.border:nth-child(7)",
          "xpath": "//label[contains(., 'Immeuble')]",
          "input_css": "label.border:nth-child(7) > input",
          "input_xpath": "//label[contains(., 'Immeuble')]//input[@type='radio']",
          "description": "Immeuble"
        }
      }
    },
    
    "localisation": {
      "description": "Champ de recherche de localisation",
      "container_css": "div.mx-auto > div > div > div.flex-1",
      "container_xpath": "//div[contains(@class, 'mx-auto')]//div[contains(@class, 'flex-1')]",
      "input_css": "input[placeholder='Ville, code postal…']",
      "input_xpath": "//input[@placeholder='Ville, code postal…']",
      "input_attributes": {
        "type": "text",
        "autocomplete": "list",
        "role": "combobox"
      },
      "suggestions_container": {
        "css_selector": "div[role='listbox']",
        "xpath": "//div[@role='listbox']",
        "items_css": "div[role='option']",
        "items_xpath": "//div[@role='option']"
      }
    }
  },

  "search_results": {
    "container": {
      "css_selector": "div.grid, div.flex",
      "xpath": "//div[contains(@class, 'grid') or contains(@class, 'flex')]",
      "description": "Container principal des résultats"
    },
    
    "articles": {
      "css_selector": "article.overflow-clip",
      "xpath": "//article[contains(@class, 'overflow-clip')]",
      "min_expected": 10,
      "wait_for": "networkidle"
    },
    
    "pagination": {
      "next_page": {
        "css_selector": "a.iad-btn[href*='page=']",
        "xpath": "//a[contains(@class, 'iad-btn') and contains(@href, 'page=')]",
        "text_pattern": "Suivant|>|»",
        "max_pages": 50
      },
      "current_page": {
        "css_selector": "a.router-link-active[href*='page=']",
        "xpath": "//a[contains(@class, 'router-link-active') and contains(@href, 'page=')]"
      }
    }
  },

  "article_list_data": {
    "reference": {
      "css_selector": "a[href*='/annonce/']",
      "xpath": ".//a[contains(@href, '/annonce/')]",
      "attribute": "href",
      "regex": "/r(\\d+)",
      "description": "Référence extraite de l'URL"
    },
    
    "titre": {
      "css_selector": "a[href*='/annonce/'] h2, a[href*='/annonce/'] h3",
      "xpath": ".//a[contains(@href, '/annonce/')]//h2 | .//a[contains(@href, '/annonce/')]//h3",
      "description": "Titre de l'annonce"
    },
    
    "lien": {
      "css_selector": "a[href*='/annonce/']",
      "xpath": ".//a[contains(@href, '/annonce/')]",
      "attribute": "href",
      "description": "Lien vers la page détail"
    },
    
    "prix": {
      "css_selector": "div p, p.text-lg, p.text-xl, strong.text-3xl",
      "xpath": ".//div//p[contains(., '€')] | .//p[contains(@class, 'text-') and contains(., '€')] | .//strong[contains(@class, 'text-3xl')]",
      "description": "Prix du bien"
    },
    
    "localisation": {
      "css_selector": "a[href*='/annonce/'] h2, a[href*='/annonce/'] h3",
      "xpath": ".//a[contains(@href, '/annonce/')]//h2 | .//a[contains(@href, '/annonce/')]//h3",
      "regex": "à\\s+(.+)",
      "description": "Localisation extraite du titre"
    },
    
    "image_url": {
      "css_selector": "img[src*='property']",
      "xpath": ".//img[contains(@src, 'property')]",
      "attribute": "src",
      "description": "URL de l'image principale"
    },
    
    "caracteristiques": {
        "css_selector": "div.flex.justify-between ul li",
        "xpath": ".//div[contains(@class, 'flex justify-between')]//ul//li",
        "multiple": true,
        "description": "Liste des caractéristiques (chambres, pièces, surface)"
    },
    
    "surface": {
        "css_selector": "div.flex.justify-between ul li",
        "xpath": ".//div[contains(@class, 'flex justify-between')]//ul//li[contains(., 'm²')]",
        "regex": "(\\d+)\\s*m²",
        "description": "Surface en m² extraite"
    },
    
    "pieces": {
        "css_selector": "div.flex.justify-between ul li",
        "xpath": ".//div[contains(@class, 'flex justify-between')]//ul//li[contains(., 'pièces')]",
        "regex": "(\\d+)",
        "description": "Nombre de pièces"
    },
    
    "chambres": {
        "css_selector": "div.flex.justify-between ul li",
        "xpath": ".//div[contains(@class, 'flex justify-between')]//ul//li[contains(., 'chambre')]",
        "regex": "(\\d+)",
        "description": "Nombre de chambres"
    },
    
    "conseiller": {
      "css_selector": "figcaption, a[href*='/conseiller-immobilier/']",
      "xpath": ".//figcaption | .//a[contains(@href, '/conseiller-immobilier/')]",
      "description": "Nom du conseiller immobilier"
    },

    
    "badges": {
        "css_selector": "span.inline, div.flex span, [class*='badge']",
        "xpath": ".//span[contains(@class, 'inline')] | .//div[contains(@class, 'flex')]//span | .//*[contains(@class, 'badge')]",
        "multiple": true,
        "description": "Badges (Nouveau, Prestige, EXCLUSIVITÉ, etc.)"
    }
  },

  "detail_page": {
    "wait_for": {
      "css_selector": "section.md\\:px-\\[4rem\\], div.lg\\:grid",
      "xpath": "//section[contains(@class, 'md:px-[4rem]')] | //div[contains(@class, 'lg:grid')]",
      "timeout": 10000
    },
    
    "data": {
      "reference": {
        "css_selector": "small.text-xs, p.pt-m",
        "xpath": "//small[contains(@class, 'text-xs')] | //p[contains(@class, 'pt-m')]",
        "regex": "(\\d+)",
        "description": "Référence de l'annonce"
      },
      
      "titre_complet": {
        "css_selector": "h1, h2.text-2xl, h2.text-3xl, h2.group-\\[\\.is-prestige\\]\\:tracking-wider",
        "xpath": "//h1 | //h2[contains(@class, 'text-')] | //h2[contains(@class, 'group-[.is-prestige]:tracking-wider')]",
        "description": "Titre complet de l'annonce"
      },
      
      "prix_detaille": {
        "css_selector": "strong.text-3xl, p.pt-xs strong",
        "xpath": "//strong[contains(@class, 'text-3xl')] | //p[contains(@class, 'pt-xs')]//strong",
        "description": "Prix détaillé"
      },
      
      "prix_au_m2": {
        "css_selector": "dl.group-\\[\\.is-prestige\\]\\:text-sm > dd:nth-child(2)",
        "xpath": "//dl[contains(@class, 'flex') and contains(@class, 'gap-m')]//dd[contains(text(), '/ m²')]",
        "description": "Prix au mètre carré"
    },
        
      "description": {
        "css_selector": "div.whitespace-pre-line",
        "xpath": "//div[contains(@class, 'whitespace-pre-line')]",
        "description": "Description complète du bien"
      },
      
      "surface": {
        "css_selector": "li:has-text('m²'), ul.pb-xl li:has-text('m²')",
        "xpath": "//li[contains(., 'm²')] | //ul[contains(@class, 'pb-xl')]//li[contains(., 'm²')]",
        "regex": "(\\d+)\\s*m²",
        "description": "Surface en m²"
      },
      
      "surface_terrain": {
        "css_selector": "li:has-text('terrain')",
        "xpath": "//li[contains(., 'terrain')]",
        "regex": "(\\d+)\\s*m²",
        "description": "Surface du terrain"
      },
      
      "pieces": {
        "css_selector": "li:has-text('pièces'), li:has-text('chambres')",
        "xpath": "//li[contains(., 'pièces') or contains(., 'chambres')]",
        "regex": "(\\d+)",
        "description": "Nombre de pièces/chambres"
      },
      
      "localisation_complete": {
        "css_selector": "h2.group-\\[\\.is-prestige\\]\\:tracking-wider",
        "xpath": "//h2[contains(@class, 'group-[.is-prestige]:tracking-wider')]",
        "description": "Localisation complète depuis le titre"
      },
      
      "caracteristiques_detaillees": {
        "css_selector": "ul.group-\\[\\.is-prestige\\]\\:grid li, ul.pb-xl li",
        "xpath": "//ul[contains(@class, 'group-[.is-prestige]:grid')]//li | //ul[contains(@class, 'pb-xl')]//li",
        "multiple": true,
        "description": "Caractéristiques détaillées du bien"
      },
      
      "equipements": {
        "css_selector": "ul.group-\\[\\.is-prestige\\]\\:grid li",
        "xpath": "//ul[contains(@class, 'group-[.is-prestige]:grid')]//li",
        "multiple": true,
        "description": "Équipements et caractéristiques spécifiques"
      },
      
      "annee_construction": {
        "css_selector": "li:has-text('Construit en')",
        "xpath": "//li[contains(., 'Construit en')]",
        "regex": "(\\d{4})",
        "description": "Année de construction"
      },
      
      "nom_conseiller": {
        "css_selector": "figure.text-prestige-secondary-40 figcaption, a.flex-col p, figure.flex figcaption",
        "xpath": "//figure[contains(@class, 'text-prestige-secondary-40')]//figcaption | //a[contains(@class, 'flex-col')]//p | //figure[contains(@class, 'flex')]//figcaption",
        "description": "Nom du conseiller immobilier"
      },
      
      "note_conseiller": {
        "css_selector": "strong.font-semiband",
        "xpath": "//strong[contains(@class, 'font-semiband') and contains(., '/5')]",
        "regex": "(\\d+/\\d)",
        "description": "Note du conseiller"
      },
      
      "nombre_avis": {
        "css_selector": "small.text-lg:has-text('avis')",
        "xpath": "//small[contains(@class, 'text-lg') and contains(., 'avis')]",
        "regex": "(\\d+)",
        "description": "Nombre d'avis du conseiller"
      },
      
      "badges": {
        "css_selector": "ul.flex li, ul.group-\\[\\.is-prestige\\]\\:justify-center li, span.inline",
        "xpath": "//ul[contains(@class, 'flex')]//li | //ul[contains(@class, 'group-[.is-prestige]:justify-center')]//li | //span[contains(@class, 'inline')]",
        "multiple": true,
        "description": "Badges (EXCLUSIVITÉ, NOUVEAU, PRESTIGE, etc.)"
      },
      
      "honoraires": {
        "css_selector": ".selecteur-qui-nexiste-pas",
        "xpath": "//div[@id='inexistant']",
        "description": "Conditions des honoraires"
      },
      
      "type_bien": {
        "css_selector": "h2.group-\\[\\.is-prestige\\]\\:tracking-wider",
        "xpath": "//h2[contains(@class, 'group-[.is-prestige]:tracking-wider')]",
        "regex": "^(Maison|Appartement|Villa|Studio|Loft|Duplex|Terrain)",
        "description": "Type de bien (extrait du titre)"
      },

      "copropriete": {
        "nb_lots": {
            "css_selector": "dl.flex.gap-m dt:has-text('Nombre de lots') + dd",
            "xpath": "//dl[contains(@class, 'flex')]//dt[contains(., 'Nombre de lots')]/following-sibling::dd[1]",
            "description": "Nombre de lots dans la copropriété"
        },
        "charges_previsionnelles": {
            "css_selector": "dl.flex.gap-m dt:has-text('Charges prévisionnelles') + dd",
            "xpath": "//dl[contains(@class, 'flex')]//dt[contains(., 'Charges prévisionnelles')]/following-sibling::dd[1]",
            "description": "Montant des charges prévisionnelles"
        },
        "procedures": {
            "css_selector": "dl.flex.gap-m dt:has-text('Procédures') + dd",
            "xpath": "//dl[contains(@class, 'flex')]//dt[contains(., 'Procédures')]/following-sibling::dd[1]",
            "description": "Informations sur les procédures en cours"
        }
      },

      "conseiller": {
        "nom_conseiller": {
          "css_selector": "a.flex-col p.text-xl",
          "xpath": "//a[contains(@class, 'flex-col')]//p[contains(@class, 'text-xl')]",
          "description": "Nom complet du conseiller"
        },
        "telephone_conseiller": {
        "css_selector": "button:has-text('+33') span",
        "xpath": "//button[contains(., '+33')]//span",
        "regex": "(\\+33[\\d\\s]+)",
        "description": "Numéro de téléphone du conseiller"
        },
        "lien_conseiller": {
          "css_selector": "a.flex-col",
          "attribute": "href",
          "xpath": "//a[contains(@class, 'flex-col')]",
          "description": "Lien vers la page du conseiller"
        },
        "photo_conseiller": {
          "css_selector": "a.flex-col img.size-full",
          "attribute": "src",
          "xpath": "//a[contains(@class, 'flex-col')]//img[contains(@class, 'size-full')]",
          "description": "URL de la photo du conseiller"
        },
        "note_conseiller": {
          "css_selector": "[class*='rating'], [class*='note']",
          "xpath": "//*[contains(@class, 'rating') or contains(@class, 'note')]",
          "description": "Note du conseiller"
        },
        "nombre_avis": {
          "css_selector": "[class*='avis'], :has-text('avis')",
          "xpath": "//*[contains(text(), 'avis') or contains(@class, 'avis')]",
          "description": "Nombre d'avis sur le conseiller"
        }
      },
      
      "images": {
        "css_selector": "img[src*='property'], figure.cursor-pointer img",
        "xpath": "//img[contains(@src, 'property')] | //figure[contains(@class, 'cursor-pointer')]//img",
        "multiple": true,
        "attribute": "src",
        "description": "URLs des images du bien (méthode basique)"
      },

      "toutes_les_photos": {
        "description": "Toutes les URLs des photos du bien en haute qualité",
        "container_css": "ul.flex-col",
        "container_xpath": "//ul[contains(@class, 'flex-col')]",
        "items_css": "ul.flex-col li img",
        "items_xpath": "//ul[contains(@class, 'flex-col')]//li//img",
        "attribute": "src",
        "multiple": true,
        "qualite_haute": true,
        "description_detaillee": "Récupère toutes les photos dans la galerie en haute qualité"
      },

      "nombre_photos": {
        "css_selector": "button.iad-btn span.font-semibold",
        "xpath": "//button[contains(@class, 'iad-btn')]//span[contains(@class, 'font-semibold')]",
        "regex": "(\\d+)\\s*photos?",
        "description": "Nombre total de photos disponibles (extrait du texte)"
      },

      "media_info": {
        "css_selector": "button.iad-btn span.font-semibold",
        "xpath": "//button[contains(@class, 'iad-btn')]//span[contains(@class, 'font-semibold')]",
        "description": "Informations complètes sur les médias (photos, vidéos, visites virtuelles)"
      },

      "has_video": {
        "css_selector": "button.iad-btn:has(span:has-text('vidéo'))",
        "xpath": "//button[contains(@class, 'iad-btn')][contains(., 'vidéo')]",
        "description": "Présence d'une vidéo"
      },

      "has_visite_virtuelle": {
        "css_selector": "button.iad-btn:has(span:has-text('visite virtuelle'))",
        "xpath": "//button[contains(@class, 'iad-btn')][contains(., 'visite virtuelle')]",
        "description": "Présence d'une visite virtuelle"
      },
      
      "date_publication": {
        "css_selector": "small.text-xs",
        "xpath": "//small[contains(@class, 'text-xs')]",
        "description": "Date de publication ou référence"
      },

      "performances_energetiques": {
        "css_selector": "section.flex",
        "xpath": "//section[contains(@class, 'flex')]",
        "description": "Section complète des performances énergétiques"
      },
      
      "titre_performances": {
        "css_selector": "section.flex > h3",
        "xpath": "//section[contains(@class, 'flex')]//h3",
        "description": "Titre 'Performances énergétiques'"
      },
      
      "dpe_description": {
        "css_selector": "div.gap-s:nth-child(1) > p:nth-child(1)",
        "xpath": "//div[contains(@class, 'gap-s')][1]//p[1]",
        "description": "Description DPE"
      },
      
      "dpe_classe_active": {
        "css_selector": "div.group-\\[\\.is-prestige\\]\\:flex-col > div:nth-child(1) li.border-white span",
        "xpath": "//div[contains(@class, 'group-[.is-prestige]:flex-col')]//div[1]//li[contains(@class, 'border-white')]//span",
        "description": "Classe DPE active (avec bordure blanche)"
      },
      
      "ges_classe_active": {
        "css_selector": "div.group-\\[\\.is-prestige\\]\\:flex-col > div:nth-child(2) li.border-white span",
        "xpath": "//div[contains(@class, 'group-[.is-prestige]:flex-col')]//div[2]//li[contains(@class, 'border-white')]//span",
        "description": "Classe GES active (avec bordure blanche)"
      },
      
      "dpe_consommation_energie_primaire": {
        "css_selector": ".border-epc-C p.flex span.text-base",
